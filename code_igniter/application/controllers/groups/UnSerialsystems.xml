		<group_padded_name></group_padded_name>
		<group_dynamic_select><![CDATA[SELECT distinct(system.id) FROM system WHERE (system.serial = '' AND system.serial = '') AND system.status = 'production' ]]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_category>general</group_category>
		<group_icon>contact</group_icon>
		<group_description>Any items that have their status attribute set to 'production' and have their serial or serial attribute set to ''.</group_description>
		<group_display_sql><![CDATA[SELECT system.id, system.hostname, system.description, system.ip_address, system.os_family, system.os_name, system.type, system.icon, system.manufacturer FROM system LEFT JOIN oa_group_sys ON system.id = oa_group_sys.id WHERE oa_group_sys.group_id = ?]]></group_display_sql>
