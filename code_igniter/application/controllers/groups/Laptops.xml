		<group_padded_name></group_padded_name>
		<group_dynamic_select><![CDATA[SELECT distinct(system.id) FROM system WHERE class = 'laptop' AND system.status = 'production']]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_category>device</group_category>
		<group_icon>computer</group_icon>
		<group_description>Any items that have their status attribute set to 'production' and have their class attribute set to 'laptop'.</group_description>
