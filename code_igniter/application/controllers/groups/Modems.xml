		<group_name>Modems</group_name>
		<group_padded_name></group_padded_name>
		<group_dynamic_select><![CDATA[SELECT distinct(system.id) FROM system WHERE system.type LIKE '%modem' AND system.status = 'production']]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_category>device</group_category>
		<group_icon>modem</group_icon>
		<group_description>Any items that have their status attribute set to 'production' and have their type attribute set to 'modem'.</group_description>
		<group_display_sql><![CDATA[SELECT system.id, system.name, system.description, system.type, system.manufacturer, system.serial, system.model, system.ip, system.asset_number FROM system, oa_group_sys WHERE system.id = oa_group_sys.system_id AND oa_group_sys.group_id = ? GROUP BY system.id ]]></group_display_sql>
