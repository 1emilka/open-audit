<?xml version="1.0" encoding="ISO-8859-1"?>
<group>
	<details>
		<group_name>Non Network Printers</group_name>
		<group_padded_name></group_padded_name>
		<group_dynamic_select><![CDATA[SELECT distinct(system.id) FROM system WHERE system.type = 'printer' AND system.status = 'production' AND (system.ip_address = '000.000.000.000' OR system.ip_address = '0.0.0.0' OR system.ip_address = '')]]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_category>device</group_category>
		<group_icon>printer</group_icon>
		<group_description>Any items that have their status attribute set to 'production' and have their type attribute set to 'printer' and have no valid ip address.</group_description>
		<group_display_sql><![CDATA[SELECT b.hostname, a.linked_sys, a.id, a.manufacturer, a.model, a.serial, a.description, a.printer_port_name, a.printer_color, a.printer_duplex, windows.user_name FROM system b LEFT JOIN windows ON (b.id = windows.id AND windows.current = 'y'), system a LEFT JOIN oa_group_sys ON a.id = oa_group_sys.id WHERE oa_group_sys.group_id = ? AND a.linked_sys = b.id GROUP BY a.id ORDER BY a.manufacturer, a.model]]></group_display_sql>
	</details>
