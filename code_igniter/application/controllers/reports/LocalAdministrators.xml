<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Local Administrators</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id, system.name, user_group.members FROM user_group LEFT JOIN system ON (user_group.id = system.id AND user_group.current = 'y') LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) WHERE oa_group_sys.group_id = @group AND LOWER(user_group.name) = "administrators"]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Users on devices in the user group 'Administrators'.</report_description>
	</details>
