<report>
	<details>
		<report_name>Specific Network</report_name>
		<report_display_in_menu>n</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id, system.hostname, system.icon, system.type, system.description, system.os_family, ip.ip AS ip, ip.network FROM system LEFT JOIN oa_group_sys ON (oa_group_sys.id = system.id) LEFT JOIN ip ON (system.id = ip.id AND ip.current = 'y') WHERE oa_group_sys.group_id = @group AND ip.network = (SELECT network FROM ip WHERE id = ? LIMIT 1)]]></report_sql>
		<report_view_file>v_report</report_view_file>
