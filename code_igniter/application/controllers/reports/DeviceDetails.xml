<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Device Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id, system.name, system.icon, system.type, DATE(system.first_seen) as first_seen, DATE(system.last_seen) as last_seen, system.last_seen_by, system.model, system.manufacturer, system.serial, system.owner, oa_location.name as location_name, system.os_family, oa_org.name as org_name FROM system LEFT JOIN oa_location ON system.location_id = oa_location.id LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) LEFT JOIN oa_org ON system.org_id = oa_org.id WHERE oa_group_sys.group_id = @group GROUP BY system.id ORDER BY system.name]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
