<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Orphaned Users</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id, system.name, user_group.name as user_group_name, user_group.members FROM system LEFT JOIN user_group ON (user_group.system_id = system.id AND user_group.current = 'y') LEFT JOIN oa_group_sys ON (system.id = oa_group_sys.system_id) WHERE oa_group_sys.group_id = @group AND user_group.members LIKE "%@,%" AND user_group.members NOT LIKE "Everyone@," GROUP BY user_group.id]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>User accounts that no longer appear to be valid.</report_description>
	</details>
	<columns>
		<column>
			<column_order>0</column_order>
			<column_name>Device Name</column_name>
			<column_variable>name</column_variable>
			<column_type>link</column_type>
			<column_link>/devices/</column_link>
			<column_secondary>id</column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
		<column>
			<column_order>1</column_order>
			<column_name>Group Name</column_name>
			<column_variable>user_group_name</column_variable>
			<column_type>text</column_type>
			<column_link></column_link>
			<column_secondary></column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
		<column>
			<column_order>1</column_order>
			<column_name>Orphaned Users</column_name>
			<column_variable>members</column_variable>
			<column_type>text</column_type>
			<column_link></column_link>
			<column_secondary></column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
	</columns>
</report>