<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>System Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id, system.name, system.os_name, system.class, system.owner, system.ip, system.model, DATE(system.first_seen) as first_seen, DATE(system.last_seen) as last_seen, system.last_seen_by, system.os_installation_date, system.manufacturer, system.serial, system.form_factor, windows.client_site_name,  windows.user_name, oa_location.name, windows.active_directory_ou FROM system LEFT JOIN oa_location ON (system.location_id = oa_location.id) LEFT JOIN windows ON (system.id = windows.system_id AND windows.current = 'y') LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.id) WHERE oa_group_sys.group_id = @group]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
