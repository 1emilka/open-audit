<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Consumed IP Addresses</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.id, system.name, system.domain, system.icon, system.os_family, ip.ip as ip, system.description FROM ip LEFT JOIN system ON (system.id = ip.id AND ip.current = 'y') LEFT JOIN oa_group_sys ON (system.id = oa_group_sys.system_id) WHERE group_id = @group AND ip.ip IS NOT NULL AND ip.ip != '127.000.000.001' AND ip.ip != '' AND ip.version = '4' GROUP BY ip.id, ip.ip ORDER BY ip.ip]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>3</report_sort_column>
		<report_description>The ip addresses used by a group.</report_description>
	</details>
